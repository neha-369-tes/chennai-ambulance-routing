# Chennai EMS N8N Integration Configuration
# Copy this file to .env and fill in your values

# Chennai EMS System
CHENNAI_EMS_BASE_URL=https://your-replit-app.replit.app
NODE_ENV=development

# N8N Authentication
N8N_API_KEY=generate-secure-random-key-here
N8N_WEBHOOK_SECRET=generate-webhook-secret-here

# Twilio Integration (for SMS/Voice alerts)
TWILIO_ACCOUNT_SID=your-twilio-account-sid
TWILIO_AUTH_TOKEN=your-twilio-auth-token
TWILIO_PHONE_NUMBER=+1234567890

# Slack Integration
SLACK_BOT_TOKEN=xoxb-your-slack-bot-token
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK

# Discord Integration (optional)
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR/DISCORD/WEBHOOK

# Email Integration (optional)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your-email@gmail.com
SMTP_PASS=your-app-password

# External Systems
TRAFFIC_API_KEY=your-traffic-api-key
WEATHER_API_KEY=your-weather-api-key